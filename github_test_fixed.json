{
    "extracted_data": {
        "name": "MkDocs Publisher",
        "description": "A simple action to build an MkDocs site and push it to the gh-pages branch.",
        "author": "DevRel Team",
        "inputs": {
            "python-version": {
                "description": "The version of Python to set up for building.",
                "required": false,
                "default": "3.11"
            },
            "requirements-file": {
                "description": "Path to the Python requirements file",
                "required": true
            },
            "gh-token": {
                "description": "GitHub token for deployment.",
                "required": true
            }
        },
        "outputs": {
            "page-url": {
                "description": "The URL of the deployed GitHub Pages site."
            }
        },
        "runs": {
            "using": "composite",
            "steps": [
                {
                    "id": "checkout",
                    "name": "Checkout Code",
                    "uses": "actions/checkout@v4"
                },
                {
                    "id": "setup_python",
                    "name": "Setup Python",
                    "uses": "actions/setup-python@v5",
                    "with": {
                        "python-version": "${{ inputs.python-version }}"
                    }
                },
                {
                    "name": "Install Python Packages",
                    "run": "pip install -r ${{ inputs.requirements-file }}",
                    "shell": "bash"
                },
                {
                    "name": "Build Site",
                    "run": "mkdocs build",
                    "shell": "bash"
                },
                {
                    "id": "deploy",
                    "uses": "peaceiris/actions-gh-pages@v3",
                    "with": {
                        "github_token": "${{ inputs.gh-token }}",
                        "publish_dir": "./site"
                    },
                    "if": "github.ref == 'refs/heads/main'"
                }
            ]
        },
        "branding": {
            "color": "blue",
            "icon": "book-open"
        }
    },
